ail},\r\n            set(value){this.$store.commit('loginEmail', value)}\r\n          },\r\n          password: {\r\n            get(){return loginData.password},\r\n            set(value){this.$store.commit('loginPassword', value)}\r\n          },\r\n        },\r\n        watch: {\r\n        },\r\n        created() {\r\n          this.isLoggedIn = this.$cookies.isKey('auth-token') \r\n        },\r\n        methods : {\r\n          ...mapActions(['login','logout']),\r\n          ...mapMutations(['setDialog', 'isLoggedInChanged']),\r\n          Login(){\r\n            this.login(this.$store.state.loginData).then(()=>this.isLoggedInChanged())\r\n          },\r\n          Logout(){\r\n            this.logout().then(() => {\r\n              this.isLoggedInChanged()\r\n              })\r\n          },\r\n          userProfile() {\r\n            console.log(this.loginData.email)\r\n            this.$router.push({name:constants.URL_TYPE.USER.PROFILE, params: {email:this.loginData.email}\r\n            })\r\n          },\r\n        },\r\n        data: function() {\r\n           return {\r\n              constants,\r\n           }\r\n        },\r\n    }\r\n</script>\r\n"]}]}