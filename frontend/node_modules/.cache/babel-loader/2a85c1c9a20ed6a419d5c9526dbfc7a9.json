IAAI,EAACgB,SAAS,CAAClC,QAAV,CAAmBmB,IAAnB,CAAwBC;AAA9B,OAAZ;AACD;AA3FM,GAxBmB;AAwH5BoB,EAAAA,OAAO,EAAE;AAxHmB,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\r\nimport Vuex from 'vuex'\r\n\r\nimport cookies from 'vue-cookies'\r\nimport axios from 'axios'\r\n\r\nimport router from '@/router'\r\nimport SERVER from '@/lib/constants'\r\n\r\nVue.use(Vuex)\r\n\r\nexport default new Vuex.Store({\r\n  state: {\r\n    // auth\r\n    authToken: cookies.get('auth-token'),\r\n\r\n    dialog : false,\r\n    isLoggedIn:false,\r\n  },\r\n  \r\n  getters: {\r\n    // auth\r\n    isLoggedIn: state => !!state.authToken,\r\n    // config : state => \r\n\r\n  },\r\n\r\n  mutations: {\r\n    SET_TOKEN(state, token) {\r\n      state.authToken = token\r\n      cookies.set('auth-token', token)\r\n    },\r\n\r\n  },\r\n\r\n  actions: {\r\n    signup({ commit }, signupData ){\r\n      const info = {\r\n        data: signupData,\r\n        location: SERVER.URL_TYPE.USER.JOIN\r\n      }\r\n      var reg = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\r\n      var passwordReg = /^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&*-]).{8,}$/;\r\n      if(!info.data.nickName){\r\n        alert('닉네임을 입력하세요')\r\n      }else if(!info.data.email){\r\n          alert('이메일을 입력하세요')\r\n      }\r\n      else if(!reg.test(info.data.email)){\r\n          alert('이메일 형식을 확인하세요')\r\n      }\r\n      else if(!info.data.password){\r\n          alert('비밀번호를 입력하세요')\r\n      }else if(!info.data.passwordConfirm){\r\n          alert('비밀번호 확인을 입력하세요')\r\n      }else if(info.data.password !== info.data.passwordConfirm){\r\n          alert('비밀번호가 일치하지 않습니다')\r\n      }else if(false === passwordReg.test(info.data.password)) {\r\n          alert('비밀번호는 8자 이상이어야 하며, 숫자/대문자/소문자/특수문자를 모두 포함해야 합니다.');\r\n      }else if(/(\\w)\\1\\1\\1/.test(info.data.password)){\r\n          alert('같은 문자를 4번 이상 사용하실 수 없습니다.');\r\n      }else if(info.data.password.search(info.data.email) > -1){\r\n          alert(\"비밀번호에 아이디가 포함되었습니다.\");\r\n      }else if(info.data.password.search(/\\s/) != -1){\r\n          alert(\"비밀번호는 공백 없이 입력해주세요.\");\r\n      }else{\r\n        axios.post(SERVER.SERVER_URL + '/account/signup', info.data)\r\n        .then(res=>{\r\n          if(res.data.data === 'emailexist'){\r\n              alert('이미 있는 이메일입니다.')\r\n          }else if(res.data.data === 'nicknameexist'){\r\n              alert('이미 있는 닉네임입니다.')\r\n          }else if(res.data.data === 'emailsuccess'){\r\n              commit('SET_TOKEN',res.data.key)\r\n            router.push({name:SERVER.URL_TYPE.POST.MAIN})\r\n          }else if(res.data.data === 'emailfail'){\r\n              alert('이메일 전송 실패')\r\n          }\r\n        })\r\n        .catch(err=>console.log(err))\r\n      }\r\n    },\r\n\r\n\r\n\r\n    login({ commit, state }, loginData){\r\n      const info = {\r\n        data: loginData,\r\n        location: SERVER.URL_TYPE.USER.LOGIN\r\n      }\r\n      var reg = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n      if (!info.data.email) {\r\n        console.log(info.data.email)\r\n        alert(\"이메일을 입력하세요\");\r\n      }\r\n      else if (!reg.test(info.data.email)) {\r\n        alert(\"이메일 형식을 확인하세요\");\r\n      }\r\n      else {\r\n      axios.post(SERVER.SERVER_URL +'/account/login/', info.data)\r\n      .then(response => {\r\n        if(response.status == 200){\r\n            commit('SET_TOKEN',res.data.key)\r\n            state.dialog = true\r\n            var jwt = require(\"jsonwebtoken\");\r\n            var token = jwt.sign({ sub: info.data.email }, info.data.password);\r\n            alert(\"로그인 성공\");\r\n            state.dialog = false\r\n            state.isLoggedIn = true\r\n            cookies.set('auth-token', token)\r\n            console.log(response);\r\n            router.push({name:constants.URL_TYPE.POST.MAIN})\r\n            router.go()\r\n          }\r\n        })\r\n        .catch((error)=>{\r\n          alert(\"로그인 실패\");\r\n          state.dialog = false;\r\n          router.push({name: 'Params', params: {name: error.response.status}});\r\n        })\r\n      } \r\n    },\r\n    logout({ commit }){\r\n      commit('SEt_TOKEN', null)\r\n      cookies.remove('auth-token')\r\n      router.push({name:constants.URL_TYPE.POST.MAIN})\r\n    },\r\n\r\n\r\n  },\r\n\r\n  modules: {\r\n  }\r\n})\r\n"]}]}